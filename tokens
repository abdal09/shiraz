 #include <stdio.h>
  int main() {
  char name [15] = "Farm House";
  char size = 'M';
  int personItServes = 2;
  float MRP = 445.8
  
  printf("%s\n" , name");
  printf("%C\n" , size);
  printf("%d\n" , personItServes);
  printf("%.1f"\n ,MRP);
  
  printf("yay! you have an order %s pizza of size %c that serves %d .\nplease pay %f to the delivery person." , name ,size , personItServes , MRP);
  
  return 0;
  }
  
